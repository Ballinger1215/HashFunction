package hashfunction;

/**
 *
 * @author Mark
 */
public class Node {
    
    public String data;
    public Node next;
    
    public String toString(){
        return data;
    }
    
    public Node(){
        data = "-1";
        next = null;
    }
    
    public Node(String newData){
        data = newData;
        next = null;
    }
    
    public Node(String newData, Node newNext){
        data = newData;
        next = newNext;
    }
}
